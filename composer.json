{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "barryvdh/laravel-ide-helper": "^2.10",
        "bavix/laravel-wallet": "*",
        "genl/matice": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "hardevine/shoppingcart": "^3.0",
        "kyslik/column-sortable": "^6.4",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "*",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.8",
        "mohammad-fouladgar/eloquent-builder": "^4.3",
        "musonza/chat": "*",
        "protonemedia/laravel-cross-eloquent-search": "^3.3",
        "rtconner/laravel-likeable": "^3.2",
        "spatie/laravel-ignition": "^2.4",
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-sitemap": "^6.1",
        "spatie/laravel-sluggable": "*",
        "spatie/laravel-tags": "^4.3",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "stevebauman/location": "^6.2",
        "tymon/jwt-auth": "^2.0",
        "willvincent/laravel-rateable": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.4",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4.4",
        "ottaviano/faker-gravatar": "^0.2.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
